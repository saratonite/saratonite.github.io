{
  "name": "saratonite.github.io",
  "version": "4.0.0",
  "description": "Sarathoite Github Page",
  "author": "Sarath &lt;sarath.hacks@gmail.com&gt;",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=dev && next start",
    "dev": "set NODE_ENV=dev && next",
    "build": "next build",
    "pro-build": "npm run initconfig && npm run export",
    "initconfig": "node ./scripts/build-config.js",
    "export": "npm run build && next export && npm run copy",
    "copy": "cp ./.gitignore ./out/ ",
    "github:export": "set NODE_ENV=github && npm run build && next export && npm run copy",
    "github:publish": "npm run github:export && node ./scripts/publish-githubpage.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^5.0.4",
    "isomorphic-fetch": "^2.2.1",
    "next": "^6.1.1",
    "node-sass": "^4.9.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-pose": "^2.1.2"
  },
  "devDependencies": {
    "@zeit/next-sass": "^0.1.2",
    "gh-pages": "^1.2.0"
  }
}
